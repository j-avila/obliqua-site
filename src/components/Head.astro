---
import cover from "../assets/BG-hero.png"
import hero from "../assets/BG-hero-desktop.png"
import logo from "../assets/logo.svg"
---

<header class="flex justify-center pt-8 md:pt-0">
  <img src={logo.src} class="absolute top-5" draggable="false" />
  <img src={hero.src} class="w-full -mt-12 hidden lg:block" draggable="false" />
  <img
    src={cover.src}
    class="w-full -mt-12 visible lg:hidden"
    draggable="false"
  />
</header>
